{
    "compileOnSave": false,
    "compilerOptions": {
      "baseUrl": ".",
      "noEmit": true,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "target": "es5",
      "paths": {
        "rxjs/*": ["node_modules/rxjs/*"]
      },
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "es2017",
        "dom"
      ]
    }
  }